<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#131313">

    <TextView
        android:id="@+id/historylbl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="60dp"
        android:fontFamily="@font/fredoka_one"
        android:text="Your Profile"
        android:textColor="#FFFFFF"
        android:textSize="48sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/historylbl3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="244dp"
        android:fontFamily="@font/fredoka_one"
        android:text="Edit Your Details"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/usertxt3"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:background="#131313"
        android:fontFamily="@font/baloo_paaji"
        android:foregroundTint="#2E2ABD"
        android:text="Username..."
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="normal"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="284dp" />

    <TextView
        android:id="@+id/passtxt3"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:fontFamily="@font/baloo_paaji"
        android:text="Password..."
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="357dp" />

    <TextView
        android:id="@+id/passtxt4"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:fontFamily="@font/baloo_paaji"
        android:text="Re-enter Password..."
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="433dp" />

    <Button
        android:id="@+id/logoutBtn"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:background="#131313"
        android:fontFamily="@font/fredoka_one"
        android:text="LOGOUT"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:ignore="TextContrastCheck"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="560dp" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView2"
        android:layout_width="415dp"
        android:layout_height="70dp"
        tools:layout_editor_absoluteY="661dp" />

</androidx.constraintlayout.widget.ConstraintLayout>