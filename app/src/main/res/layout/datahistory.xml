<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#131313"
    android:stretchColumns="1">

    <FrameLayout
        android:id="@+id/FL3"
        android:layout_width="349dp"
        android:layout_height="250dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:background="#202020"
        app:layout_constraintBottom_toTopOf="@+id/FL4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Historylbl">

        <TableLayout
            android:id="@+id/table"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow>
                <TextView
                    android:layout_column="1"
                    android:padding="10dip"
                    android:text="Date"
                    android:textColor="#E91E63" />

                <TextView
                    android:layout_width="116dp"
                    android:layout_height="39dp"
                    android:gravity="center"
                    android:padding="3dip"
                    android:text="Time"
                    android:textColor="#E91E63" />

                <TextView
                    android:layout_width="120dp"
                    android:layout_height="50dp"
                    android:gravity="right"
                    android:padding="3dip"
                    android:text="Potassium level (mEq/L)"
                    android:textColor="#E91E63" />
            </TableRow>

            <View
                android:layout_height="2dip"
                android:background="#FF909090" />

            <TableRow>
                <TextView
                    android:layout_column="1"
                    android:padding="10dip"
                    android:text="12/12/2022"
                    android:textColor="#2196F3" />

                <TextView
                    android:layout_width="116dp"
                    android:layout_height="39dp"
                    android:gravity="center"
                    android:padding="3dip"
                    android:text="06:48:03"
                    android:textColor="#2196F3" />

                <TextView
                    android:gravity="right"
                    android:padding="3dip"
                    android:text="3.67"
                    android:textColor="#2196F3" />
            </TableRow>

            <View
                android:layout_height="2dip"
                android:background="#FF909090" />

            <TableRow>
                <TextView
                    android:layout_column="1"
                    android:padding="10dip"
                    android:text="13/12/2022"
                    android:textColor="#2196F3" />

                <TextView
                    android:layout_width="116dp"
                    android:layout_height="39dp"
                    android:gravity="center"
                    android:padding="3dip"
                    android:text="06:34:45"
                    android:textColor="#2196F3" />

                <TextView
                    android:gravity="right"
                    android:padding="3dip"
                    android:text="3.23"
                    android:textColor="#2196F3" />
            </TableRow>

            <View
                android:layout_height="2dip"
                android:background="#FF909090"/>

            <TableRow>
                <TextView
                    android:layout_column="1"
                    android:padding="10dip"
                    android:text="14/12/2022"
                    android:textColor="#2196F3" />

                <TextView
                    android:layout_width="116dp"
                    android:layout_height="39dp"
                    android:gravity="center"
                    android:padding="3dip"
                    android:text="07:02:53"
                    android:textColor="#2196F3" />

                <TextView
                    android:gravity="right"
                    android:padding="3dip"
                    android:text="5.38"
                    android:textColor="#2196F3" />
            </TableRow>
        </TableLayout>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/FL4"
        android:layout_width="349dp"
        android:layout_height="250dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="20dp"
        android:background="#202020"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationViewDH"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FL3">

    </FrameLayout>

    <TextView
        android:id="@+id/Historylbl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="60dp"
        android:fontFamily="@font/fredoka_one"
        android:text="History"
        android:textColor="#FFFFFF"
        android:textSize="48sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationViewDH"
        android:layout_width="415dp"
        android:layout_height="70dp"
        app:itemBackground="@color/black"
        app:itemIconSize="40dp"
        app:itemIconTint="@color/white"
        app:itemTextColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/navbar" />

</androidx.constraintlayout.widget.ConstraintLayout>